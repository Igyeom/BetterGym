{
    "manifest_version": 3,
    "name": "BetterGym",
    "version": "0.1",
    "author": "ians.site",
    "description": "An extension that improves the language-gym.com experience, made proudly by Igyeom \"Ian\" Park (ians.site) :D",
    "icons": {
        "16": "icons/16a.png",
        "48": "icons/48a.png",
        "128": "icons/128a.png"
    },
    "action": {
        "default_popup": "html/config.html"
    },
    "content_scripts": [
        {
            "matches": ["https://language-gym.com/game/*"],
            "js": ["js/script.js"]
        }
    ]
}